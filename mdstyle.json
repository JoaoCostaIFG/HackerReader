{
  "document": {
    "color": "#EEEEEE",
    "margin": 0
  },
  "block_quote": {
    "indent_token": "│ "
  },
  "paragraph": {},
  "list": {
    "level_indent": 2
  },
  "heading": {
    "block_suffix": "\n"
  },
  "h1": {
    "prefix": "# "
  },
  "h2": {
    "prefix": "## "
  },
  "h3": {
    "prefix": "### "
  },
  "h4": {
    "prefix": "#### "
  },
  "h5": {
    "prefix": "##### "
  },
  "h6": {
    "prefix": "###### "
  },
  "text": {},
  "strikethrough": {
    "crossed_out": true
  },
  "emph": {
    "italic": true
  },
  "strong": {
    "bold": true
  },
  "hr": {
    "format": "---"
  },
  "item": {
    "block_prefix": "- "
  },
  "enumeration": {
    "block_prefix": ". "
  },
  "task": {
    "ticked": "[x] ",
    "unticked": "[ ] "
  },
  "link": {
    "underline": true,
    "block_prefix": "(",
    "block_suffix": ")"
  },
  "link_text": {},
  "image": {},
  "image_text": {
    "format": "Image: {{.text}} →"
  },
  "code": {
    "block_prefix": "`",
    "block_suffix": "`"
  },
  "code_block": {
    "margin": 2
  },
  "table": {
    "center_separator": "┼",
    "column_separator": "│",
    "row_separator": "─"
  },
  "definition_list": {},
  "definition_term": {},
  "definition_description": {},
  "html_block": {},
  "html_span": {}
}
